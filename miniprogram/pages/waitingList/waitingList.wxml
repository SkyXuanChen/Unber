<!--pages/waitingList/waitingList.wxml-->
<view class="page-wrapper ">
  <view class="calendar-bar-wrapper js_calendar_wrapper">
    <view class="bus-tabs calendar-bar">
      <view class="tabs-item js_pre_day" ontap="preDay" >排序栏</view>
      <view ontap="showCalendar" class="tabs-item js_show_calendar" style="-webkit-flex: 2; flex: 2;">{{calendarSelectedDateStr}}</view>
      <view class="tabs-item js_next_day" ontap="nextDay">排序栏</view>
    </view>
  </view>
  <view class="bus-list js_bus_list ">

    <block wx:for="{{listData}}" wx:key="k" wx:for-list_id="id" bindtap="onNamepiece">
      <view class="bus-list-item">
        <view class="bus-time">{{item.id}}</view>
        <view class="tobooking" id="{{item.id}}" bindtap="onMatch"> 匹配 </view>
        <view class="detail">
          <view class="detail1">
            <view class="start">
              <text class="icon-circle s-icon1"></text>{{item.address}}</view>
            <view class="end">
              <text class="icon-circle s-icon2"></text>{{item.destiny}}</view>
          </view>
          <view class="tags">
            <view>
              <text class="price">{{item.latitude}}</text>
            </view>
            <view>
              <text class="countleft">{{item.longitude}}</text>
            </view>
            <view>
              <text class="b-tags js_tags"></text>
            </view>
          </view>
        </view>
      </view>

    </block>

  </view>

</view>